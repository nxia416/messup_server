<h1>Listing tcpdump_logs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>User data</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tcpdump_logs.each do |tcpdump_log| %>
      <tr>
        <td><%= tcpdump_log.user_id %></td>
        <td><%= tcpdump_log.user_data %></td>
        <td><%= link_to 'Show', tcpdump_log %></td>
        <td><%= link_to 'Edit', edit_tcpdump_log_path(tcpdump_log) %></td>
        <td><%= link_to 'Destroy', tcpdump_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tcpdump log', new_tcpdump_log_path %>
